<p id="notice"><%= notice %></p>

<%= render @photo %>

<section>
  <%= render @photo.comments, photo: @photo %>
</section>

<% if current_user == @photo.user %>
  <%= link_to 'Edit', edit_photo_path(@photo) %> ·
  <%= link_to 'Destroy', @photo, method: :delete, data: { confirm: 'Are you sure?' } %> ·
<% end %>
<%= link_to 'Back', :back %>
